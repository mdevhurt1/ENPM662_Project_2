{
    "name": "ROS2 Galactic Dev Environment",
    "dockerFile": "Dockerfile",
    "workspaceFolder": "/workspace",
    "mounts": [
      "source=${localWorkspaceFolder},target=/workspace,type=bind,consistency=cached"
    ],
    "runArgs": [
      "--gpus=all",
      "--net=host",
      "--privileged",
      "--env=DISPLAY=${env:DISPLAY}"
    ],
    "remoteUser": "root"
  }
